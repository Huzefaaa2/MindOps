{
  "id": null,
  "uid": "ebpf-bot-telemetry",
  "title": "eBPF Bot Telemetry",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Decisions Per Second",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(ebpf_bot_decisions_total[1m])",
          "legendFormat": "{{component}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Decision Latency (p50/p95/p99)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(ebpf_bot_decision_latency_ms_bucket[1m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(ebpf_bot_decision_latency_ms_bucket[1m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(ebpf_bot_decision_latency_ms_bucket[1m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    }
  ],
  "templating": {
    "list": []
  }
}
